{
  "name": "seek-app",
  "version": "1.0.0",
  "keywords": [
    "react"
  ],
  "description": "Create React apps with no build configuration.",
  "repository": {
    "type": "git",
    "url": "https://github.com/aicode2080/seek-app.git",
    "directory": "packages/create-seek-app"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0 <23.0.0"
  },
  "bugs": {
    "url": "https://github.com/aicode2080/seek-app/issues"
  },
  "files": [
    "lib",
    "index.js",
    "README.md",
    "LICENSE",
    "template",
    "code-template"
  ],
  "bin": {
    "react-module": "./lib/index.js",
    "ga": "./lib/git-auto.js",
    "async": "./lib/interface-auto.js",
    "lint": "",
    "seek-app": "./lib/seek-app.js"
  },
  "scripts": {
    "addModule": "node ./lib/index.js --name react-module",
    "addRedux": "node ./lib/create-redux.js --name redux-module",
    "search": "node ./lib/search-code.js",
    "git": "node ./lib/git-auto.js",
    "interface": "node ./lib/interface-auto.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "seek": "node ./lib/seek-app.js",
    "build": "rollup -c",
    "clean": "rimraf lib"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "axios": "^0.21.1",
    "chalk": "^3.0.0",
    "child_process": "^1.0.2",
    "commander": "^4.1.1",
    "cross-spawn": "^7.0.3",
    "envinfo": "^7.8.1",
    "eslint": "^9.23.0",
    "fs-extra": "^9.1.0",
    "hyperquest": "^2.1.3",
    "inquirer": "^7.3.3",
    "json-schema-to-typescript": "^10.0.0",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "prompts": "^2.4.2",
    "semver": "^7.3.5",
    "tar-pack": "^3.4.1",
    "tmp": "^0.1.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@vitest/coverage-v8": "^0.34.6",
    "cross-env": "^7.0.3",
    "jest": "^27.4.3",
    "postcss": "^8.4.31",
    "postcss-px-to-viewport": "^1.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "vitest": "^0.34.6"
  }
}
